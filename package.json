{
  "name": "reporting-service",
  "version": "0.0.1",
  "private": true,
  "main": "dist",
  "scripts": {
    "dev": "nodemon $NODE_DEBUG_OPTION -w src --exec \"babel-node src --presets env,stage-0\"",
    "build": "babel src -s -D -d dist --presets env,stage-0",
    "start": "node dist",
    "test": "node_modules/.bin/mocha --require babel-polyfill --require babel-register --exit --recursive --timeout 5000"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "dependencies": {
    "async": "^2.6.0",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "body-parser": "^1.18.2",
    "btoa": "^1.2.1",
    "cookie-parser": "^1.4.3",
    "eslint-plugin-import": "^2.10.0",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "express-validator": "^4.3.0",
    "helmet": "^3.12.0",
    "jsonpath": "^1.0.0",
    "keycloak-connect": "^4.0.0-beta.1",
    "keycloak-request-token": "^0.1.0",
    "moment": "^2.22.0",
    "morgan": "^1.9.0",
    "nsp": "^3.2.1",
    "querystring": "^0.2.0",
    "requirejs": "^2.3.5",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.9.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-2": "^6.24.1",
    "babelify": "^8.0.0",
    "browserify": "^16.2.0",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "eslint": "^4.19.1",
    "expect": "^22.4.3",
    "mocha": "^5.0.5",
    "nock": "^9.2.3",
    "node-mocks-http": "^1.6.7",
    "nodemon": "^1.12.1",
    "proxyquire": "^2.0.1",
    "sinon": "^4.5.0"
  }
}
